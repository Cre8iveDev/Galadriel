<h1>{{ section.settings.title }}</h1>
<p>{{ section.settings.subtext_title | t }}</p>

{% schema %}
{
    "name": "Template 404",
    "settings": [
        {
            "type": "text",
            "id": "title",
            "default": "Error 404",
            "label": "404 Heading"
        },
        {
            "type": "text",
            "id": "subtext_title",
            "default": "The page you're looking for is not found",
            "label": "404 Subtext"
        }
    ]
}
{% endschema %}