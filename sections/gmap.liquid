<section class="gmap map-one">
    <iframe
        src="{{ section.settings.gmap_url }}"
        class="google-map__contact" allowfullscreen></iframe>
    <div class="container">
        <div class="row">
            {% comment %}
            <div class="col-lg-4">
                <div class="map-one__single wow fadeInUp " data-wow-duration="1500ms">
                    <h3 class="map-one__title">United Kingdom</h3><!-- /.map-one__title -->
                    <ul class="map-one__list list-unstyled">
                        <li><i class="fa fa-map-marker"></i> 35 Street, Deerfield Valdosta Road, <br> United
                            Kingdom
                        </li>
                        <li><i class="fa fa-envelope"></i>Inquiry@creatic.com </li>
                        <li><i class="fa fa-phone"></i>2800 256 508 </li>
                    </ul><!-- /.map-one__list -->
                </div><!-- /.map-one__single -->
            </div><!-- /.col-lg-4 -->
            <div class="col-lg-4">
                <div class="map-one__single wow fadeInUp" data-wow-duration="1500ms">
                    <h3 class="map-one__title">Australia</h3><!-- /.map-one__title -->
                    <ul class="map-one__list list-unstyled">
                        <li><i class="fa fa-map-marker"></i> 35 Street, Deerfield Valdosta Road, <br> United
                            Kingdom
                        </li>
                        <li><i class="fa fa-envelope"></i>Inquiry@creatic.com </li>
                        <li><i class="fa fa-phone"></i>2800 256 508 </li>
                    </ul><!-- /.map-one__list -->
                </div><!-- /.map-one__single -->
            </div><!-- /.col-lg-4 -->
            <div class="col-lg-4">
                <div class="map-one__single wow fadeInUp" data-wow-duration="1500ms">
                    <h3 class="map-one__title">United State</h3><!-- /.map-one__title -->
                    <ul class="map-one__list list-unstyled">
                        <li><i class="fa fa-map-marker"></i> 35 Street, Deerfield Valdosta Road, <br> United
                            Kingdom
                        </li>
                        <li><i class="fa fa-envelope"></i>Inquiry@creatic.com </li>
                        <li><i class="fa fa-phone"></i>2800 256 508 </li>
                    </ul><!-- /.map-one__list -->
                </div><!-- /.map-one__single -->
            </div><!-- /.col-lg-4 -->
            {% endcomment %}
        </div><!-- /.row -->
    </div><!-- /.container -->
</section><!-- /.map-one -->

